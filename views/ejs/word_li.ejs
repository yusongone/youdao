<li class="one_word">
	<span class="key"><%-word.word%><i class="fa fa-plus-square-o"></i></span>
	<div class="trans">
		<%if(word.trans.basic){%>	
		<ul class="basic">
			<%for(var i=0;i<word.trans.basic.explains.length;i++){ var explains=word.trans.basic.explains[i];%>
				<li>
					<span><%-explains%></span>
				</li>
			<%}%>
		</ul>
		<%}%>
		<%if(word.trans.web){%>	
		<ul class="web">
			<%for(var i=0;i<word.trans.web.length;i++){ var web=word.trans.web[i];%>
				<li>
					<span class="web_key"><%-web.key%>:</span>
					<span><%-web.value%></span>
				</li>
			<%}%>
		</ul>
		<%}%>
	<div>
</li>
